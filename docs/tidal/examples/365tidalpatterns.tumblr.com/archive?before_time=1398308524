|+| speed "[1 0.8 0.5 1.2]/12"
|+| delay ((*0.7) &lt;$&gt; slow 3.75 sinewave1)
|+| delaytime ((*0.4) &lt;$&gt; slow 3.1 sinewave1)
|+| delayfeedback ((*0.9) &lt;$&gt; slow 4.8 sinewave1)
