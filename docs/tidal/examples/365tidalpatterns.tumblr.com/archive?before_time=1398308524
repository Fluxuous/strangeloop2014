</div><div class="post post_micro is_regular post_tumblelog_c4ea9f2fbf633a20ea74c4654eb48a08 not_mine is_original with_permalink no_source no_notes has_hover" id="post_micro_82965016355"  data-id='82965016355' data-type='regular' data-root_id='82965016355' data-tumblelog='365tidalpatterns' data-tumblelog_key='YCGbdmhef' data-reblog_key='giNNvBPK' data-is_mine='' data-accepts_answers='' data-following_tumblelog='' data-liked='' data-sponsored='' data-premium_tracked='' data-is_recommended='' data-placement_id='' data-featured_tags='Array' data-reblog_source='' data-post-id='82965016355' data-root-id='82965016355' data-tumblelog-name='365tidalpatterns' data-tumblelog-key='YCGbdmhef' data-reblog-key='giNNvBPK' data-serve-id='0.67768900 1410718451742138102' data-json='{"id":"82965016355","type":"regular","root_id":"82965016355","tumblelog":"365tidalpatterns","tumblelog_key":"YCGbdmhef","reblog_key":"giNNvBPK","is_mine":false,"accepts_answers":false,"following_tumblelog":false,"liked":false,"sponsored":false,"premium_tracked":false,"is_recommended":false,"placement_id":null,"featured_tags":[],"reblog_source":null,"post-id":"82965016355","root-id":"82965016355","tumblelog-name":"365tidalpatterns","tumblelog-key":"YCGbdmhef","reblog-key":"giNNvBPK","serve-id":"0.67768900 1410718451742138102","json":"{\u0022id\u0022:\u002282965016355\u0022,\u0022type\u0022:\u0022regular\u0022,\u0022root_id\u0022:\u002282965016355\u0022,\u0022tumblelog\u0022:\u0022365tidalpatterns\u0022,\u0022tumblelog_key\u0022:\u0022YCGbdmhef\u0022,\u0022reblog_key\u0022:\u0022giNNvBPK\u0022,\u0022is_mine\u0022:false,\u0022accepts_answers\u0022:false,\u0022following_tumblelog\u0022:false,\u0022liked\u0022:false,\u0022sponsored\u0022:false,\u0022premium_tracked\u0022:false,\u0022is_recommended\u0022:false,\u0022placement_id\u0022:null,\u0022featured_tags\u0022:[],\u0022reblog_source\u0022:null,\u0022post-id\u0022:\u002282965016355\u0022,\u0022root-id\u0022:\u002282965016355\u0022,\u0022tumblelog-name\u0022: