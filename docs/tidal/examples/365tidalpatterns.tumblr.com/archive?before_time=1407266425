<pre><code>bps (172/120)

d1 $ slowspread ($) [striate 2 . slow 2, jux (iter 8), 
(|+| speed "0.9"), slow 1, slow 2, (|+| accelerate "-0.2"), 
slow 1, trunc 0.25 , trunc 0.25] $ every 3 (0.25 &lt;~) $ 
every 4 (0.25 &lt;~) $ stack [slow 2 $ every 4 (const $ 
sound (samples "sub ~ sub b ~ ~ b ~" (run 29)) |+| cut "1") 
$ sound (samples "~ [~ b] ~ b ~ b [~ b?] ~" (run 18)) 
|+| cut "1", density 2 $ sound (samples "{~ ~ ~ ~ ~ ~ ~ ~, 
hh hh hh hh ~ ~}" (run 13)) |+| gain "0.8", slow 2 $ 
whenmod 5 4 (const $ sound (samples 
"nerd kh*2 psh bass3 kit*4 doru*4 untuned*2 kit2*2" 
(run 31))) $ sound (samples "kh [psh ~ heem perc] 
[~ psh] ~ [psh ~ doru perc] [untuned psh] [doru kit2] [psh kit1]" 
(run 28)) ] |+| shape "0.2"
</code></pre></div></div><div class="post_glass post_micro_glass" 
