|+| speed ((+0.5) &lt;$&gt; slow 1.8 sine1), sound (samples 
"[[kh*2 pain]/2 earth] [shard:9 pain/2 earth kh/2]" 
(slow 0.9 $ run 12)) |+| shape "0.2", sound (samples 
"~ ~ ~ b ~ ~ [~ b] [~ b]" (run 47)) |+| cut "2" |+| speed "2", 
