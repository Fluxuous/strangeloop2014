<pre><code>bps (180/120)

d1 $ whenmod 40 35 (slow 2 . (|+| speed "0.75")) $ 
whenmod 22 20 (density 2) $ whenmod 20 18 (density 2) $ 
whenmod 32 16 (slow 1.5) $ slowspread ($) [(|+| speed "0.5") . 
slow 2, within (0.25, 0.75) (rev . (|+| speed "1.2")), 
(|+| accelerate "-0.5, 0.5"), id, trunc 0.25, trunc 0.25, chop 8 . rev, 
(|+| accelerate "-1 1")] $ whenmod 14 12 (const $ stack [ 
sound "kh:12*16" |+| gain "0.8", sound "slayer:8*4 slayer:9*4" 
|+| cut "1"]) $ every 3 (0.25 &lt;~) $ every 4 (0.25 &lt;~) $ stack [ 
degradeBy 0.4 $ sound (samples "slayer*8" "[7 9 10 11 6]/6") 
|+| cut "1" |+| speed "[1 1 1 0.8]/5", whenmod 8 4 (degradeBy 0.5) 
$ sound "kh:12*16" |+| gain "0.8", every 5 (density 2) $ every 4 (
density 2) $ every 3 (density 2) $ every 2 (density 2) $ sound 
"hh:26*4" ]
</code></pre></div></div><div class="post_glass post_micro_glass" 
