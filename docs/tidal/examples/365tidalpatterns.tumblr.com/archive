stack [ slow 2 $ slowspread (chop) [128, 64, 256, 32, 200] $ 
sound (samples 
"newnotes*4 [~ keys2*2 keys2*2 keys2*2] [keys1 keys1 
newnotes newnotes] newnotes*4" (run 33)) |+| cutoff 
(scale 0.03 0.3 $ slow 2.9 sine1) |+| resonance "0.7" |+| cut "1" 
|+| speed "1, 1.5" |+| shape "0.3" |+| delay "0.4", slow 2 $ 
sound (samples "kh*8 [shard kv ~ kv] [kv kl ~ kl] [shard laz:0 ~ gab:0]" 
(whenmod 9 6 (slow 0.8) $ whenmod 7 5 (0.25 ~&gt;) $ run 43)), 
slow 1.5 $ sound "fuckable" |+| speed "1.1" |+| delay "0.3" 
|+| shape "0.5"] |+| delaytime "0.4" |+| delayfeedback "0.3"
</code></pre></div></div><div class="post_glass post_micro_glass" 
