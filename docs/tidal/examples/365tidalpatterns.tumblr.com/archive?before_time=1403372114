samples "[b [~ b] ~ b*2 sub ~ b ~]/5" (slow 0.8 $ run 21)) 
|+| cut "1" |+| speed "[1 1.25 1 -1 1 1.25 1 0.8]/3", 
sound (samples "[~ vox ~ llama ~ josie ~ fran ~ control]/9" 
(slow 0.8 $ run 33)) |+| speed "1" |+| cut "2" ]
