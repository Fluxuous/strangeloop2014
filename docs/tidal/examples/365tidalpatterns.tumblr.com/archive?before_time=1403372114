<pre><code>bps (125/120)

d1 $ whenmod 6 5 (0.25 &lt;~) $ slowspread ($) [ 
slow 2, slow 1, trunc 0.25, slow 1, slow 1] $ stack [ 
sound "tin ~ tin tin" |+| gain "0.9" |+| cut "2", sound 
"[pada pada:1]/2" |+| cut "1" |+| speed "[1 0.9]/2", 
sound (samples "kh*16?" "7") |+| gain "0.85", slow 2 $ 
sound (samples "[dup hh]*2 ~ [s cp] ~ [~ dup dup cp] [stite 
[laz:3 ~]/2?] s hh*8" (run 12))] |+| accelerate "[0 0.5 0 -0.5]/4" 
|+| shape "0.3"
