|+| speed "[1 0.8 1 [1 -1.1]/2]/3", slow 2 $ sound (samples 
"[~ sub] ~ ~ [sub ~]/2 ~ ~ b ~" (every 5 (slow 0.7) $ run 52)) 
|+| cut "1", slow 6 $ (striate 16 $ sound "fxlong") 
|+| speed "[[1 1.2 1.5]/2, [~ 0.8 0.5]]" |+| gain "0.7" ] 
|+| shape "0.2"
</code></pre></div></div><div class="post_glass post_micro_glass" 
